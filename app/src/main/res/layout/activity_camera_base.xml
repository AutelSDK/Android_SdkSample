<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    tools:context=".RCActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="100px"
        android:background="@drawable/text_background"
        android:gravity="center"
        android:padding="@dimen/text_padding"
        android:text="setCameraConnectStateListener" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/setCameraConnectStateListener"
            android:layout_width="wrap_content"
            android:layout_height="100px"
            android:background="@drawable/text_background"
            android:gravity="center"
            android:padding="@dimen/text_padding"
            android:text="set" />

        <TextView
            android:id="@+id/resetCameraConnectStateListener"
            android:layout_width="wrap_content"
            android:layout_height="100px"
            android:background="@drawable/text_background"
            android:gravity="center"
            android:padding="@dimen/text_padding"
            android:text="reset" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/item_margin_bottom"
        android:orientation="vertical">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="100px"
            android:background="@drawable/text_background"
            android:gravity="center"
            android:padding="@dimen/text_padding"
            android:text="setCameraSDCardStateListener" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/setCameraSDCardStateListener"
                android:layout_width="wrap_content"
                android:layout_height="100px"
                android:background="@drawable/text_background"
                android:gravity="center"
                android:padding="@dimen/text_padding"
                android:text="set" />

            <TextView
                android:id="@+id/resetCameraSDCardStateListener"
                android:layout_width="wrap_content"
                android:layout_height="100px"
                android:background="@drawable/text_background"
                android:gravity="center"
                android:padding="@dimen/text_padding"
                android:text="reset" />
        </LinearLayout>
    </LinearLayout>


    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/item_margin_bottom"
        android:orientation="vertical">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="100px"
            android:background="@drawable/text_background"
            android:gravity="center"
            android:padding="@dimen/text_padding"
            android:text="MediaModeListener" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/setAutelCameraModeListener"
                android:layout_width="wrap_content"
                android:layout_height="100px"
                android:background="@drawable/text_background"
                android:gravity="center"
                android:padding="@dimen/text_padding"
                android:text="set" />

            <TextView
                android:id="@+id/resetAutelCameraModeListener"
                android:layout_width="wrap_content"
                android:layout_height="100px"
                android:background="@drawable/text_background"
                android:gravity="center"
                android:padding="@dimen/text_padding"
                android:text="reset" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/item_margin_bottom"
        android:orientation="vertical">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="100px"
            android:background="@drawable/text_background"
            android:gravity="center"
            android:padding="@dimen/text_padding"
            android:text="setCameraMediaStateListener" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/setCameraMediaStateListener"
                android:layout_width="wrap_content"
                android:layout_height="100px"
                android:background="@drawable/text_background"
                android:gravity="center"
                android:padding="@dimen/text_padding"
                android:text="set" />

            <TextView
                android:id="@+id/resetCameraMediaStateListener"
                android:layout_width="wrap_content"
                android:layout_height="100px"
                android:background="@drawable/text_background"
                android:gravity="center"
                android:padding="@dimen/text_padding"
                android:text="reset" />
        </LinearLayout>
    </LinearLayout>

    <TextView
        android:id="@+id/formatSDCard"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/item_margin_bottom"
        android:background="@drawable/text_background"
        android:gravity="center"
        android:padding="@dimen/text_padding"
        android:text="formatSDCard" />

    <TextView
        android:id="@+id/resetCamera"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/item_margin_bottom"
        android:background="@drawable/text_background"
        android:gravity="center"
        android:padding="@dimen/text_padding"
        android:text="resetDefaults" />


    <TextView
        android:id="@+id/getWorkStatus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/item_margin_bottom"
        android:background="@drawable/text_background"
        android:gravity="center"
        android:padding="@dimen/text_padding"
        android:text="getWorkStatus" />


    <TextView
        android:id="@+id/getSDCardStatus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/item_margin_bottom"
        android:background="@drawable/text_background"
        android:gravity="center"
        android:padding="@dimen/text_padding"
        android:text="getSDCardStatus" />


    <TextView
        android:id="@+id/getSdFreeSpace"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/item_margin_bottom"
        android:background="@drawable/text_background"
        android:gravity="center"
        android:padding="@dimen/text_padding"
        android:text="getSDCardFreeSpace" />


    <TextView
        android:id="@+id/getNickName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/item_margin_bottom"
        android:background="@drawable/text_background"
        android:gravity="center"
        android:padding="@dimen/text_padding"
        android:text="getNickName" />


    <TextView
        android:id="@+id/getProduct"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/item_margin_bottom"
        android:background="@drawable/text_background"
        android:gravity="center"
        android:padding="@dimen/text_padding"
        android:text="getProduct" />

    <TextView
        android:id="@+id/getTimeStamp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/item_margin_bottom"
        android:background="@drawable/text_background"
        android:gravity="center"
        android:padding="@dimen/text_padding"
        android:text="getTimeStamp" />


    <TextView
        android:id="@+id/getVersion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/item_margin_bottom"
        android:background="@drawable/text_background"
        android:gravity="center"
        android:padding="@dimen/text_padding"
        android:text="getVersion" />


    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/item_margin_bottom"
        android:orientation="vertical">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="100px"
            android:background="@drawable/text_background"
            android:gravity="center"
            android:padding="@dimen/text_padding"
            android:text="setMediaMode" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/setMediaModeSINGLE"
                android:layout_width="wrap_content"
                android:layout_height="100px"
                android:background="@drawable/text_background"
                android:gravity="center"
                android:padding="@dimen/text_padding"
                android:text="SINGLE" />

            <TextView
                android:id="@+id/setMediaModeVIDEO"
                android:layout_width="wrap_content"
                android:layout_height="100px"
                android:background="@drawable/text_background"
                android:gravity="center"
                android:padding="@dimen/text_padding"
                android:text="VIDEO" />

            <TextView
                android:id="@+id/setMediaModeTIMELAPSE"
                android:layout_width="wrap_content"
                android:layout_height="100px"
                android:background="@drawable/text_background"
                android:gravity="center"
                android:padding="@dimen/text_padding"
                android:text="TIMELAPSE" />

            <TextView
                android:id="@+id/setMediaModeBURST"
                android:layout_width="wrap_content"
                android:layout_height="100px"
                android:background="@drawable/text_background"
                android:gravity="center"
                android:padding="@dimen/text_padding"
                android:text="BURST" />

            <TextView
                android:id="@+id/setMediaModeAEB"
                android:layout_width="wrap_content"
                android:layout_height="100px"
                android:background="@drawable/text_background"
                android:gravity="center"
                android:padding="@dimen/text_padding"
                android:text="AEB" />
        </LinearLayout>
    </LinearLayout>

    <TextView
        android:id="@+id/getMediaMode"
        android:layout_width="wrap_content"
        android:layout_height="100px"
        android:layout_marginBottom="@dimen/item_margin_bottom"
        android:background="@drawable/text_background"
        android:gravity="center"
        android:padding="@dimen/text_padding"
        android:text="getMediaMode" />


    <TextView
        android:id="@+id/takePhoto"
        android:layout_width="wrap_content"
        android:layout_height="100px"
        android:layout_marginBottom="@dimen/item_margin_bottom"
        android:background="@drawable/text_background"
        android:gravity="center"
        android:padding="@dimen/text_padding"
        android:text="startTakePhoto" />


    <TextView
        android:id="@+id/startRecordVideo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/item_margin_bottom"
        android:background="@drawable/text_background"
        android:gravity="center"
        android:padding="@dimen/text_padding"
        android:text="startRecordVideo" />

    <TextView
        android:id="@+id/stopRecordVideo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/item_margin_bottom"
        android:background="@drawable/text_background"
        android:gravity="center"
        android:padding="@dimen/text_padding"
        android:text="stopRecordVideo" />

    <TextView
        android:id="@+id/stopTimelapse"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/item_margin_bottom"
        android:background="@drawable/text_background"
        android:gravity="center"
        android:padding="@dimen/text_padding"
        android:text="stopTakePhoto(Timelapse)" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:padding="@dimen/text_padding"
        android:text="-------$$$$$ base camera end $$$$$------" />
</LinearLayout>
